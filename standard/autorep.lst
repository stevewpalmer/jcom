      180 a$(14):="      PRINT a$(t)(i:i),                           "
      190 a$(15):="    ENDIF                                         "
      200 a$(16):="  ENDFOR i                                        "
      210 a$(17):="  PRINT quote$                                    "
      220 a$(18):="ENDFOR t                                          "
      230 a$(19):="FOR t:=after TO line                              "
      240 a$(20):="  PRINT a$(t)                                     "
      250 a$(21):="ENDFOR t                                          "
      260 FOR t:=1 TO pre DO
      270   PRINT a$(t)
